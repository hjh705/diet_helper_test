<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="semitest">
    <select id="findAll" resultType="questionboard">
        select
        *
        from
        semitest
        order by
        reg_date desc
    </select>
</mapper>

